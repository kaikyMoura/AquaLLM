generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

enum MeasureType {
  WATER
  GAS
}

model tb_measures {
  measure_uuid String  @id
  image_url String
  measure_value Int
  confirmed_value Int?
  measure_datetime DateTime
  measure_type MeasureType
  customer_code String
  has_confirmed Boolean
}